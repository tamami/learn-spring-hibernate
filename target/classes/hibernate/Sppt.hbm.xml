<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD.3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="lab.aikibo.entity.Sppt" table="sppt_terhutang">
    <property name="nop" type="string">
      <column name="nop" />
    </property>
    <property name="thnPajak" type="string">
      <column name="thn" />
    </property>
    <property name="nama" type="string">
      <column name="nama" />
    </property>
    <property name="alamatOp"  type="string">
      <column name="alamat_op" />
    </property>
    <property name="pokok" type="big_integer">
      <column name="pokok" />
    </property>
    <property name="denda" type="big_integer">
      <column name="denda" />
    </property>
  </class>
</hibernate-mapping>
